
local ReplicatedStorage = game:GetService("ReplicatedStorage")

-- Function to get item information
local function GetItemInfo(ItemsClass, ItemsName)
    local Inventory = require(ReplicatedStorage:WaitForChild("Library"):WaitForChild("Client").Save).Get().Inventory
    for _, Item in pairs(Inventory[ItemsClass]) do
        if table.find(ItemsName, Item.id) and Item._am then
            return Item._am
        end
    end
    return nil
end

-- Search for Insta Plant Capsule and Diamond
local function CheckResources()
    local capsuleAmount = GetItemInfo("Misc", {"Insta Plant Capsule"}) or 0
    local diamondAmount = GetItemInfo("Seed", {"Diamond"}) or 0

    print("Insta Plant Capsule: " .. capsuleAmount)
    print("Diamonds: " .. diamondAmount)

    return capsuleAmount, diamondAmount
end

-- Function to claim mail gifts
local function MailClaim()
    while true do
        local player = game.Players.LocalPlayer
        local gui = player.PlayerGui:FindFirstChild("_MACHINES")
        if gui then
            local mailboxMachine = gui:FindFirstChild("MailboxMachine")
            if mailboxMachine then
                local giftsFrame = mailboxMachine:FindFirstChild("Frame") and mailboxMachine.Frame:FindFirstChild("GiftsFrame")
                if giftsFrame then
                    local itemsFrame = giftsFrame:FindFirstChild("ItemsFrame")
                    if itemsFrame then
                        local frameChild = itemsFrame:FindFirstChildWhichIsA("Frame")
                        if frameChild then
                            local args = {
                                [1] = {
                                    [1] = frameChild.Name
                                }
                            }
                            ReplicatedStorage:WaitForChild("Network"):WaitForChild("Mailbox: Claim"):InvokeServer(unpack(args))
                        end
                    end
                end
            end
        end
        wait(1)
    end
end

-- Function to plant seeds
local function PlantSeeds()
    while true do
        for i = 1, 11 do
            local args = {
                [1] = "FlowerGarden",
                [2] = "PlantSeed",
                [3] = i,
                [4] = "Diamond"
            }
            ReplicatedStorage.Network.Instancing_InvokeCustomFromClient:InvokeServer(unpack(args))
            wait()
        end
    end
end

-- Function to claim plants
local function ClaimPlants()
    while true do
        for i = 1, 11 do
            local args = {
                [1] = "FlowerGarden",
                [2] = "ClaimPlant",
                [3] = i
            }
            ReplicatedStorage.Network.Instancing_FireCustomFromClient:FireServer(unpack(args))
            wait()
        end
    end
end

-- Function to grow plants
local function GrowPlants()
    while true do
        for i = 1, 11 do
            local args = {
                [1] = "FlowerGarden",
                [2] = "InstaGrowSeed",
                [3] = i
            }
            ReplicatedStorage.Network.Instancing_InvokeCustomFromClient:InvokeServer(unpack(args))
            wait()
        end
    end
end
