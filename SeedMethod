                    }
                    game:GetService("ReplicatedStorage").Network.Instancing_InvokeCustomFromClient:InvokeServer(unpack(args))
                    wait()
                end
            end
        end)()
    end

    local function GetItemAmount()
        local Inventory = require(ReplicatedStorage:WaitForChild("Library"):WaitForChild("Client").Save).Get().Inventory
        for _, Item in pairs(Inventory["Misc"]) do
            if Item.id == "Seed Bag" and Item._am then
                return Item._am
            end
        end
        return nil
    end

    repeat
        local seedBagAmount = GetItemAmount()
        if seedBagAmount then
            print("Seed Bags: " .. seedBagAmount)
        else
          seedBagAmount = 0
        end
        wait(0.1)
    until seedBagAmount < 100

