wait(300)
repeat
    wait(0.1)
    local royalPurpleFound = false
    for _, child in ipairs(activeBlocks:GetChildren()) do
        if child:IsA("Part") and child.BrickColor == BrickColor.new("Royal purple") then
            royalPurpleFound = true
            break  -- Stop the loop if we find any Royal purple part
        end
    end
until not royalPurpleFound  -- Repeat until no Royal purple parts are found

local Http = game:GetService("HttpService")
local request = (syn and syn.request) or request or (http and http.request) or http_request

-- Fetch unique server data
local response = request({
    Url = 'https://bots-juzzoualive.replit.app/uniqueServers',
    Method = 'GET',
    Headers = {
        ['Content-Type'] = 'application/json'
    }
})

local data = Http:JSONDecode(response.Body)
local servers = data.uniqueServers
local mergedServers = {} -- Table to hold all server IDs (merged)

-- Add all servers to mergedServers
for _, server in ipairs(servers) do
    table.insert(mergedServers, server)
end

local PlaceID = game.PlaceId
local AllIDs = {} -- Stores the server IDs that have already been used
local foundAnything = ""
local actualHour = os.date("!*t").hour

-- Load previously saved server IDs
local File = pcall(function()
    AllIDs = Http:JSONDecode(readfile("NotSameServers.json"))
end)

if not File then
    table.insert(AllIDs, actualHour)
    writefile("NotSameServers.json", Http:JSONEncode(AllIDs))
end

function TPReturner()
    local Site
    if foundAnything == "" then
        Site = Http:JSONDecode(game:HttpGet('https://games.roblox.com/v1/games/' .. PlaceID .. '/servers/Public?sortOrder=Asc&limit=100'))
    else
        Site = Http:JSONDecode(game:HttpGet('https://games.roblox.com/v1/games/' .. PlaceID .. '/servers/Public?sortOrder=Asc&limit=100&cursor=' .. foundAnything))
    end

    if Site.nextPageCursor and Site.nextPageCursor ~= "null" and Site.nextPageCursor ~= nil then
        foundAnything = Site.nextPageCursor
    end

    local serverIDs = {} -- Table to hold valid server IDs
    for _, v in pairs(Site.data) do
        local ID = tostring(v.id)
        if tonumber(v.maxPlayers) > tonumber(v.playing) then
            local alreadyUsed = false

            -- Ensure the server is not already in the mergedServers or AllIDs list
            if table.find(mergedServers, ID) or table.find(AllIDs, ID) then
                alreadyUsed = true
            end

            if not alreadyUsed then
                table.insert(AllIDs, ID)
                table.insert(serverIDs, ID)
                if #serverIDs >= 20 then -- Collect up to 20 IDs
                    break
                end
            end
        end
    end

    -- Attempt to teleport to a valid server
    if #serverIDs > 0 then
        while #serverIDs > 0 do
            local randomIndex = math.random(1, #serverIDs)
            local selectedServerID = serverIDs[randomIndex]

            -- Check if the server is in mergedServers
            if not table.find(mergedServers, selectedServerID) then
                -- Teleport to the valid server
                pcall(function()
                    writefile("NotSameServers.json", Http:JSONEncode(AllIDs))
                    game:GetService("TeleportService"):TeleportToPlaceInstance(PlaceID, selectedServerID, game.Players.LocalPlayer)
                end)
                break
            else
                -- Remove invalid server ID and try again
                table.remove(serverIDs, randomIndex)
            end
        end
    end
end

function Teleport()
    while wait() do
        pcall(function()
            TPReturner()
            if foundAnything ~= "" then
                TPReturner()
            end
        end)
    end
end

-- Start the teleport process
while true do
    Teleport()
    wait(15) -- Wait 15 seconds before checking again
end

